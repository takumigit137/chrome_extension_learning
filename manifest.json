{
    "name": "chrome-extension-learning",
    "description": "If you access Qiita, warn about not registering",
    "version": "1.0.0",
    "manifest_version":3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "my_icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://qiita.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}